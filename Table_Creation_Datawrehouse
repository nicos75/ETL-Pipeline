### Creation of the Fact table FACT_KLINES ###
### This section is an extract of all the codes deployed to load and transform data ###
create or replace TABLE EXAM_DATASCIENTEST.EXAM_DATASCIENTEST_SCHEMA.FACT_KLINES (
	SYMBOL_ID NUMBER(38,0) NOT NULL,
	SYMBOLS VARCHAR(16777216) NOT NULL,
	KLINE_OPEN_TIME TIMESTAMP_NTZ(9) NOT NULL,
	OPEN_PRICE NUMBER(38,8),
	HIGH_PRICE NUMBER(38,20),
	LOW_PRICE NUMBER(38,20),
	CLOSE_PRICE NUMBER(38,8),
	VOLUME NUMBER(38,8),
	KLINE_CLOSE_TIME TIMESTAMP_NTZ(9),
	QUOTE_ASSET_VOLUME NUMBER(38,8),
	NUMBER_OF_TRADES NUMBER(38,0),
	TAKER_BUY_BASE_ASSET_VOLUME NUMBER(38,8),
	TAKER_BUY_QUOTE_ASSET_VOLUME NUMBER(38,8),
	primary key (SYMBOLS)
